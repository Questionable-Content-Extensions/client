{
    "name": "questionable-content-spa",
    "version": "0.7.0",
    "description": "Questionable Content Single-Page Application with Extra Features",
    "private": true,
    "scripts": {
        "start": "cross-env PORT=8124 BROWSER=none FAST_REFRESH=false WDS_SOCKET_HOST=localhost EXTEND_ESLINT=true react-app-rewired start",
        "build": "node build",
        "react-build": "react-app-rewired build",
        "debug-build": "react-app-rewired --inspect-brk=5858 build",
        "test": "react-app-rewired test",
        "eject": "react-scripts eject",
        "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006 -s public --no-open",
        "build-storybook": "build-storybook -s public",
        "prepare": "husky install"
    },
    "author": "Alexander Krivács Schrøder",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/Questionable-Content-Extensions/client/issues"
    },
    "homepage": "https://questionablextensions.net/",
    "devDependencies": {
        "@commitlint/cli": "^17.7.1",
        "@commitlint/config-conventional": "^17.7.0",
        "@storybook/addon-actions": "^6.5.10",
        "@storybook/addon-essentials": "^6.5.10",
        "@storybook/addon-interactions": "^6.5.10",
        "@storybook/addon-links": "^6.5.10",
        "@storybook/builder-webpack5": "^6.5.10",
        "@storybook/jest": "^0.0.10",
        "@storybook/manager-webpack5": "^6.5.10",
        "@storybook/node-logger": "^6.5.10",
        "@storybook/preset-create-react-app": "^4.1.2",
        "@storybook/react": "^6.5.10",
        "@storybook/testing-library": "^0.0.13",
        "@trivago/prettier-plugin-sort-imports": "^4.2.0",
        "@types/react-color": "^3.0.6",
        "@types/redux-logger": "^3.0.9",
        "autoprefixer": "^10.4.8",
        "babel-plugin-named-exports-order": "^0.0.2",
        "cross-env": "^7.0.3",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.3",
        "postcss": "^8.4.16",
        "prettier": "2.5.1",
        "prop-types": "^15.8.1",
        "tailwindcss": "^3.3.3",
        "tsconfig-paths-webpack-plugin": "^4.1.0",
        "typescript": "^4.9.5",
        "typescript-plugin-css-modules": "^3.4.0",
        "webpack": "^5.74.0"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.7.2",
        "@types/greasemonkey": "^4.0.2",
        "@types/node": "^16.11.21",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "react": "^17.0.2",
        "react-app-rewired": "^2.1.8",
        "react-color": "^2.19.3",
        "react-dom": "^17.0.2",
        "react-redux": "^7.0.0",
        "react-scripts": "5.0.0",
        "redux-logger": "^3.0.6",
        "use-between": "^1.0.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write"
        ],
        "**/*.{js,jsx,ts,tsx}": [
            "eslint --fix"
        ]
    }
}
